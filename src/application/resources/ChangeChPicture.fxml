              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Pane fx:id="chPicturePane1" styleClass="pane">
                     <children>
                        <ImageView fx:id="chPicture1" fitHeight="127.0" fitWidth="145.0" layoutX="5.0" layoutY="5.0" onMouseClicked="#selectChPicture" pickOnBounds="true" />
                     </children>
                  </Pane>
                  <Pane fx:id="chPicturePane2" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.columnIndex="1">
                     <children>
                        <ImageView fx:id="chPicture2" fitHeight="127.0" fitWidth="145.0" layoutX="5.0" layoutY="5.0" onMouseClicked="#selectChPicture" pickOnBounds="true" />
                     </children>
                  </Pane>
                  <Pane fx:id="chPicturePane3" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.rowIndex="1">
                     <children>
                        <ImageView fx:id="chPicture3" fitHeight="127.0" fitWidth="145.0" layoutX="5.0" layoutY="5.0" onMouseClicked="#selectChPicture" pickOnBounds="true" />
                     </children>
                  </Pane>
                  <Pane fx:id="chPicturePane4" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <children>
                        <ImageView fx:id="chPicture4" fitHeight="127.0" fitWidth="145.0" layoutX="5.0" layoutY="5.0" onMouseClicked="#selectChPicture" pickOnBounds="true" />
                     </children>
                  </Pane>
               </children>
            </GridPane>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Button mnemonicParsing="false" prefHeight="31.0" prefWidth="122.0" text="Ok" />
                  <Button mnemonicParsing="false" onMouseReleased="#closeChPicture" prefHeight="31.0" prefWidth="122.0" text="Back" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
