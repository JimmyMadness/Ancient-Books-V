         <right>
            <ImageView fitHeight="84.0" fitWidth="81.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@images/warning.jpg" />
               </image>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </ImageView>
         </right>
         <center>
            <VBox alignment="CENTER_LEFT" prefHeight="233.0" prefWidth="332.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="popupLabel1" text="Label" visible="false">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="popupLabel2" text="Label" visible="false">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="popupLabel3" text="Label" visible="false">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="popupLabel4" text="Label" visible="false">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="popupLabel5" text="Label" visible="false">
